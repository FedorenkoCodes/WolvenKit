<UserControl
    x:Class="WolvenKit.Views.RedValueEditors.CNumberEditor"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:WolvenKit.Views.RedValueEditors"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:redTypes="clr-namespace:WolvenKit.App.ViewModels.Shell.RedTypes;assembly=WolvenKit.App"
    xmlns:syncfusion="http://schemas.syncfusion.com/wpf"
    d:DataContext="{d:DesignInstance redTypes:CUInt64ViewModel}"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <syncfusion:UpDown
        Margin="5,0,0,0"
        Padding="0"
        Background="Transparent"
        BorderThickness="0"
        IsScrollingOnCircle="False"
        MaxValue="{Binding MaxValue, Mode=OneWay}"
        MinValue="{Binding MinValue, Mode=OneWay}"
        NumberDecimalDigits="{Binding DecimalDigits, Mode=OneWay}"
        Value="{Binding BindingValue}" />
</UserControl>
