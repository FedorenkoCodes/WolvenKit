<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:redValueEditors="clr-namespace:WolvenKit.Views.RedValueEditors">
    <redValueEditors:RedValueEditorSelector x:Key="RedValueEditorSelector">
        <redValueEditors:RedValueEditorSelector.CArrayEditor>
            <DataTemplate>
                <redValueEditors:CArrayEditor />
            </DataTemplate>
        </redValueEditors:RedValueEditorSelector.CArrayEditor>
        <redValueEditors:RedValueEditorSelector.CBoolEditor>
            <DataTemplate>
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="1" />
                        <ColumnDefinition Width="*" />
                    </Grid.ColumnDefinitions>

                    <TextBlock
                        Grid.Column="0"
                        Margin="5,0"
                        VerticalAlignment="Center"
                        Text="{Binding PropertyName, Mode=OneTime}" />

                    <GridSplitter
                        Grid.Column="1"
                        HorizontalAlignment="Stretch"
                        Background="{StaticResource BorderAlt}" />

                    <redValueEditors:CBoolView
                        Grid.Column="2"
                        Margin="5,0"
                        VerticalAlignment="Center" />
                </Grid>
            </DataTemplate>
        </redValueEditors:RedValueEditorSelector.CBoolEditor>
        <redValueEditors:RedValueEditorSelector.CDictionaryEditor>
