<UserControl
    x:Class="WolvenKit.Views.RedValueEditors.CLegacySingleChannelCurveEditor"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:Syncfusion="http://schemas.syncfusion.com/wpf"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:WolvenKit.Views.RedValueEditors"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:redTypes="clr-namespace:WolvenKit.App.ViewModels.Shell.RedTypes;assembly=WolvenKit.App"
    d:DataContext="{d:DesignInstance redTypes:CLegacySingleChannelCurveViewModel}"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <Button
        x:Name="CurveEditorButton"
        Syncfusion:FocusManagerHelper.FocusedElement="True"
        Click="CurveEditorButton_OnClick"
        Content="Open Curve Editor" />
</UserControl>
